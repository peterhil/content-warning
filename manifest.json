{
    "manifest_version": 3,
    "name": "ReSort",
    "version": "0.1",
    "description": "Re-sort lists alphabetically",
    "permissions": ["storage", "tabs"],
    "host_permissions": [
        "https://*.youtube.com/*",
        "https://*.wikipedia.org/*"
   ],
    "content_scripts" : [{
        "matches" : [
            "https://*.youtube.com/*",
            "https://*.wikipedia.org/*"
        ],
        "js" : ["content.js"]
    }]
}
